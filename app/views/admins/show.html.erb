<!--特定ユーザーの管理画面-->
<h1><%= @user.name %></h1> 
    <% @prescriptions.each do |prescription| %>
        <%= prescription.start_time.strftime("%Y年%m月%d日") %><br>
        <%= prescription.visit_date.strftime("%Y年%m月%d日") %><br>
        <%= prescription.hospital %><br>
        <%= prescription.doctor %><br>
        <%= prescription.pharmacy %><br>
        <%= prescription.created_at %><br>
        <%= prescription.updated_at %><br>
        <%= prescription.updated_at %><br>
    <% end %>